Пример настройки гита
1) выполнить
    git config --global credential.helper store

2) войти в гит через token
    git pull

3) настроить конфиг файл

[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
[remote "origin"]
	url = git@github.com:monstrikus/lang-zavgar-mobile.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "develop"]
	remote = origin
	merge = refs/heads/develop
[branch "master"]
	remote = origin
	merge = refs/heads/master
[credential]
	helper = store
[user]
	name = Monstrikus
	email = kalugin666@yandex.ru
